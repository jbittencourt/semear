<%= error_messages_for :user %>

<% form_for @map do |f| %>
	<%= f.error_messages :class=>'errorbox'%>

	<% rounded_box :blue do |b| %>
		<p><label for="map_question"><%= "What is the question that you wants to investigate with this map?" %></label>
		<%= f.text_field :question %></p>
				
		<p><%= f.submit "Create Map" %></p>
	<% end %>
	
<% end %>